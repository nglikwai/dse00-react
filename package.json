  "scripts": {
    "debug:development": "cp .env.development .env && DEBUG=axios NODE_ENV=development ts-node server.js",
    "debug:staging": "cp .env.staging .env && DEBUG=axios NODE_ENV=development ts-node server.js",
    "debug:production": "cp .env.production .env && DEBUG=axios NODE_ENV=development ts-node server.js",
    "dev": "NODE_ENV=development node --inspect --require ts-node/register server.js",
    "build": "next build",
    "bump:master": "standard-version --releaseCommitMessageFormat 'chore(master): bump to {{currentTag}}\n\n[skip master-ci]' --no-verify",
    "bump:release": "standard-version --releaseCommitMessageFormat 'chore(release): bump to {{currentTag}}\n\n[skip release-ci]' --no-verify --prerelease beta-0 --skip.changelog=true --skip.tag=true",
    "test": "jest --verbose",
    "lint": "tsc && next lint",
    "postinstall": "patch-package",
    "prepare": "husky install"
  },
